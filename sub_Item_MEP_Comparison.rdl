  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Balfour_Planning_System</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ItemID">
            <Value>=Parameters!ItemID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        ISNULL(tCatalog.Description, tProject.Description) AS Description, ISNULL(tCatalog.Model, tProject.Model) AS Model, ISNULL(tCatalog.Electrical_Description, 
                         tProject.Electrical_Description) AS Electrical_Description, ISNULL(tCatalog.Special_Instructions, tProject.Special_Instructions) AS Special_Instructions, 
                         ISNULL(tCatalog.Manu_Name, tProject.Manu_Name) AS Manu_Name, ISNULL(tCatalog.Volts, tProject.Volts) AS Volts, ISNULL(tCatalog.Hz, tProject.Hz) AS Hz, 
                         ISNULL(tCatalog.Amps, tProject.Amps) AS Amps, ISNULL(tCatalog.Phase, tProject.Phase) AS Phase, ISNULL(tCatalog.Width, tProject.Width) AS Width, 
                         ISNULL(tCatalog.Height, tProject.Height) AS Height, ISNULL(tCatalog.Depth, tProject.Depth) AS Depth, ISNULL(tCatalog.Mounting, tProject.Mounting) AS Mounting, 
                         tbl_Linked_Items.Item_ID
FROM            tbl_Linked_Items LEFT OUTER JOIN
                             (SELECT        tbl_Items.ID, tbl_Items.Description, tbl_Items.Model, tbl_Items.Electrical_Description, tbl_Items.Special_Instructions, 
                                                         tbl_Manufacturers_1.Manu_Name, tbl_Items.Volts, tbl_Items.Hz, tbl_Items.Amps, tbl_Items.Phase, tbl_Items.Width, tbl_Items.Height, tbl_Items.Depth, 
                                                         vw_Ref_Table_1.VT AS Mounting
                               FROM            tbl_Manufacturers AS tbl_Manufacturers_1 INNER JOIN
                                                         tbl_Items ON tbl_Manufacturers_1.ID = tbl_Items.Manufacturer_ID LEFT OUTER JOIN
                                                         vw_Ref_Table AS vw_Ref_Table_1 ON tbl_Items.Mounting_ID = vw_Ref_Table_1.ID) AS tProject ON tbl_Linked_Items.Child_Item_ID = tProject.ID AND 
                         tbl_Linked_Items.IsCatalogItem = 0 LEFT OUTER JOIN
                             (SELECT        tbl_Catalog_Items.ID, tbl_Catalog_Items.Description, tbl_Catalog_Items.Model, tbl_Catalog_Items.Electrical_Description, 
                                                         tbl_Catalog_Items.Special_Instructions, tbl_Manufacturers.Manu_Name, tbl_Catalog_Items.Volts, tbl_Catalog_Items.Hz, tbl_Catalog_Items.Amps, 
                                                         tbl_Catalog_Items.Phase, tbl_Catalog_Items.Width, tbl_Catalog_Items.Height, tbl_Catalog_Items.Depth, vw_Ref_Table.VT AS Mounting
                               FROM            tbl_Manufacturers INNER JOIN
                                                         tbl_Catalog_Items ON tbl_Manufacturers.ID = tbl_Catalog_Items.Manufacturer_ID LEFT OUTER JOIN
                                                         vw_Ref_Table ON tbl_Catalog_Items.Mounting_ID = vw_Ref_Table.ID) AS tCatalog ON tCatalog.ID = tbl_Linked_Items.Child_Item_ID AND 
                         tbl_Linked_Items.IsCatalogItem = 1
WHERE        (tbl_Linked_Items.Item_ID = @ItemID)</CommandText>
      </Query>
