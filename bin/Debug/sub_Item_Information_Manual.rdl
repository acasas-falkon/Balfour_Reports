        <CommandText>SELECT        tbl_Items.ID, ISNULL(tbl_Room_Items.New_Qty, 0) AS New_Qty, ISNULL(tbl_Room_Items.Existing_Qty, 0) AS Existing_Qty, 
                         ISNULL(tbl_Room_Items.Future_Qty, 0) AS Future_Qty, ISNULL(tbl_Room_Items.Lease_Qty, 0) AS Lease_Qty, ISNULL(tbl_Room_Items.Growth_Qty, 0) 
                         AS Growth_Qty, ISNULL(tbl_Room_Items.Upgrade_Qty, 0) AS Upgrade_Qty, tbl_Rooms.Room_Num, tbl_Rooms.Room_Name, tbl_Departments.Dept_Num, 
                         tbl_Departments.Dept_Name, tbl_Items.Internal_Item_Num
FROM            tbl_Items INNER JOIN
                         tbl_Room_Items ON tbl_Items.ID = tbl_Room_Items.Item_ID INNER JOIN
                         tbl_Rooms ON tbl_Room_Items.Room_ID = tbl_Rooms.ID INNER JOIN
                         tbl_Departments ON tbl_Rooms.Department_ID = tbl_Departments.ID
WHERE        (tbl_Items.ID = @ID)</CommandText>
      </Query>
