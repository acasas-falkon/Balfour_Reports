                         ISNULL(tbl_Room_Items.Future_Qty, 0) AS Future_Qty, ISNULL(tbl_Room_Items.Lease_Qty, 0) AS Lease_Qty, ISNULL(tbl_Room_Items.Growth_Qty, 0) 
                         AS Growth_Qty, ISNULL(tbl_Room_Items.Upgrade_Qty, 0) AS Upgrade_Qty, tbl_Rooms.Room_Num, tbl_Rooms.Room_Name, tbl_Departments.Dept_Num, 
                         tbl_Departments.Dept_Name, tbl_Items.Internal_Item_Num
FROM            tbl_Items INNER JOIN
                         tbl_Room_Items ON tbl_Items.ID = tbl_Room_Items.Item_ID INNER JOIN
                         tbl_Rooms ON tbl_Room_Items.Room_ID = tbl_Rooms.ID INNER JOIN
                         tbl_Departments ON tbl_Rooms.Department_ID = tbl_Departments.ID
WHERE        (tbl_Items.ID = @ID)</CommandText></Query><Fields><Field Name="ID"><DataField>ID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="New_Qty"><DataField>New_Qty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Existing_Qty"><DataField>Existing_Qty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Future_Qty"><DataField>Future_Qty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Lease_Qty"><DataField>Lease_Qty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Growth_Qty"><DataField>Growth_Qty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Upgrade_Qty"><DataField>Upgrade_Qty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Room_Num"><DataField>Room_Num</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Room_Name"><DataField>Room_Name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Dept_Num"><DataField>Dept_Num</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Dept_Name"><DataField>Dept_Name</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:T