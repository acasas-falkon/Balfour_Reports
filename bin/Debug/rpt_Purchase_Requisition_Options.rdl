eryParameters><QueryParameter Name="@RoomItemID"><Value>=Parameters!RoomItemID.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        tbl_Item_Options.Room_Item_ID, tbl_Item_Options.Quantity AS Option_Qty, tbl_Item_Options.Description AS Option_Description, tbl_Item_Options.Model, 
                         CASE WHEN Item_ID IS NULL THEN tbl_Item_Options.Unit_Dollars ELSE NULL END AS Unit_Dollars, vw_Ref_Table.VT AS Option_Type, CASE WHEN Item_ID IS NULL THEN 1 ELSE 0 END AS RIOption
FROM            tbl_Item_Options INNER JOIN
                         vw_Ref_Table ON tbl_Item_Options.Option_ID = vw_Ref_Table.ID
WHERE        (tbl_Item_Options.Room_Item_ID = @RoomItemID)
UNION
SELECT        tbl_Room_Items.ID AS Room_Item_ID, 1 AS Option_Qty, NULL AS Option_Description, NULL AS Model, tbl_Req_Room_Items.Adjustment AS Unit_Dollars, 
                         'Adjustment' AS Option_Type, 2 AS RIOption
FROM            tbl_Req_Room_Items INNER JOIN
                         tbl_Room_Items ON tbl_Req_Room_Items.Room_ID = tbl_Room_Items.Room_ID AND tbl_Req_Room_Items.Item_ID = tbl_Room_Items.Item_ID
WHERE        (tbl_Room_Items.ID = @RoomItemID) AND (tbl_Req_Room_Items.Adjustment &lt;&gt; 0)
ORDER BY RIOption</CommandText></Query><Fields><Field Name="Room_Item_ID"><DataField>Room_Item_ID</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Option_Qty"><DataField>Option_Qty</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field><Field Name="Option_Description"><DataField>Option_Description</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Model"><DataField>Model</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="Unit_Dollars"><DataField>Unit_Dollars</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Decimal</rd:TypeName></Field><Field Name="Option_Type"><DataField>Option_Type</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.String</rd:TypeName></Field><Field Name="RIOption"><DataField>RIOption</DataField><rd:TypeName xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">System.Int32</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="RoomItemID"><DataType>String</DataType><Prompt>Room Item ID</Prompt></ReportParameter></ReportParameters><Language>en-US</Language><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">Inch</rd:ReportUnitType><rd:ReportID xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">f220b979-24bf-4699-a8d6-7c9d81992c9c</rd:ReportID></Report>