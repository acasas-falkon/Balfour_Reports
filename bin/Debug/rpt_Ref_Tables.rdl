  <DataSets>
    <DataSet Name="Balfour_Planning_System">
      <Query>
        <DataSourceName>Balfour_Planning_System</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Header_ID">
            <Value>=Parameters!Header_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        tbl_Reference_Header.Field_Name, vw_Ref_Table.VT, vw_Ref_Table.Sort
FROM            vw_Ref_Table INNER JOIN
                         tbl_Reference_Header ON vw_Ref_Table.Header_ID = tbl_Reference_Header.ID
WHERE        (vw_Ref_Table.Header_ID = @Header_ID)</CommandText>
      </Query>
      <Fields>
        <Field Name="Field_Name">
