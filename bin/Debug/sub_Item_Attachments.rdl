DataSourceID></DataSource></DataSources><DataSets><DataSet Name="Balfour_Planning_System"><Query><DataSourceName>Balfour_Planning_System</DataSourceName><QueryParameters><QueryParameter Name="@ItemID"><Value>=Parameters!ItemID.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        tbl_File_Info.Link_ID, tbl_Files.Stored_File, tbl_Files.MIMEType, tbl_File_Info.PEM_PDF_ID
FROM            tbl_File_Info INNER JOIN
                         tbl_Files ON tbl_File_Info.ID = tbl_Files.File_Info_ID
WHERE        (tbl_File_Info.Table_ID = 7) AND (tbl_File_Info.Prints = 1) AND (tbl_File_Info.Thumbnail = 0) AND (tbl_File_Info.Link_ID = @ItemID) AND 
