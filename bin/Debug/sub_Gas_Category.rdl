  <DataSets>
    <DataSet Name="Balfour_Planning_System">
      <Query>
        <DataSourceName>Balfour_Planning_System</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Item_ID">
            <Value>=Parameters!Item_ID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        vw_Ref_Table.VT, vw_Ref_Table.Header_ID
FROM            vw_Ref_Table INNER JOIN
                             (SELECT        Item_ID, Category_ID
                               FROM            tbl_Items_Gas
                               WHERE        (Item_ID = @Item_ID)) AS Category ON vw_Ref_Table.ID = Category.Category_ID
WHERE        (vw_Ref_Table.Header_ID = 6)
ORDER BY vw_Ref_Table.Sort</CommandText>
      </Query>
      <Fields>
        <Field Name="VT">
          <DataField>VT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Header_ID">
          <DataField>Header_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
