      <Query>
        <DataSourceName>Balfour_Planning_System</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        tbl_Departments.ID, tbl_Departments.Dept_Name, tbl_Rooms.Room_Name, tbl_Room_Items.New_Qty, tbl_Items.Internal_Item_Num, tbl_Items.Description
FROM            tbl_Departments INNER JOIN
                         tbl_Rooms ON tbl_Departments.ID = tbl_Rooms.Department_ID INNER JOIN
                         tbl_Room_Items ON tbl_Rooms.ID = tbl_Room_Items.Room_ID INNER JOIN
                         tbl_Items ON tbl_Room_Items.Item_ID = tbl_Items.ID
WHERE        (tbl_Departments.Project_ID = 1)
ORDER BY tbl_Departments.Dept_Name, tbl_Rooms.Room_Name, tbl_Items.Internal_Item_Num</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
