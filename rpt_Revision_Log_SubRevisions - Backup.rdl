        </Field>
        <Field Name="Room_Item_ID">
          <DataField>Room_Item_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="RevisionType">
          <DataField>RevisionType</DataField>
