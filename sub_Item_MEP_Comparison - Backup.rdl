      <Query>
        <DataSourceName>Balfour_Planning_System</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        tbl_Linked_Items.Item_ID, tbl_Catalog_Items.Description, tbl_Catalog_Items.Model, tbl_Catalog_Items.Electrical_Description, 
                         tbl_Catalog_Items.Special_Instructions, tbl_Manufacturers.Manu_Name, tbl_Catalog_Items.Volts, tbl_Catalog_Items.Hz, tbl_Catalog_Items.Amps, 
                         tbl_Catalog_Items.Phase, tbl_Catalog_Items.Width, tbl_Catalog_Items.Height, tbl_Catalog_Items.Depth
FROM            tbl_Manufacturers INNER JOIN
                         tbl_Catalog_Items ON tbl_Manufacturers.ID = tbl_Catalog_Items.Manufacturer_ID INNER JOIN
                         tbl_Linked_Items ON tbl_Catalog_Items.ID = tbl_Linked_Items.Catalog_Item_ID
WHERE        (tbl_Linked_Items.Item_ID = @ItemID)
ORDER BY tbl_Linked_Items.OptionOrder</CommandText>
        <QueryParameters>
          <QueryParameter Name="@ItemID">
            <Value>=Parameters!ItemID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
