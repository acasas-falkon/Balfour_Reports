      <Query>
        <DataSourceName>Balfour_Planning_System</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        tbl_Projects.ID, tItemType.VT AS ItemType, tbl_Manufacturers.Manu_Name, tbl_Rooms.Room_Num, tbl_Requisitions.RequiredPODate, tbl_Requisitions.PO_Number, 
                         tbl_Req_Room_Items.WarrantyBeginDate, tbl_Items.Model, tbl_Req_Room_Items.WarrantyEndDateLabor, tbl_Req_Room_Items.WarrantyEndDateParts, 
                         tbl_Projects.ContractorHandlingFee, dbo.fnExtCostCalc(tbl_Req_Room_Items.Purchase_Price, NULL, NULL, vw_Item_Options.Option_Price, 
                         vw_RI_Options.Option_Price, tbl_Req_Room_Items.Adjustment, tbl_Room_Items.New_Qty) AS Calc_Price
FROM            tbl_Projects INNER JOIN
                         tbl_Requisitions ON tbl_Projects.ID = tbl_Requisitions.Project_ID INNER JOIN
                         tbl_Req_Room_Items ON tbl_Requisitions.ID = tbl_Req_Room_Items.Req_ID INNER JOIN
                         tbl_Items ON tbl_Req_Room_Items.Item_ID = tbl_Items.ID INNER JOIN
                         tbl_Room_Items ON tbl_Req_Room_Items.Room_ID = tbl_Room_Items.Room_ID AND tbl_Req_Room_Items.Item_ID = tbl_Room_Items.Item_ID INNER JOIN
                         tbl_Departments ON tbl_Projects.ID = tbl_Departments.Project_ID INNER JOIN
                         tbl_Rooms ON tbl_Room_Items.Room_ID = tbl_Rooms.ID AND tbl_Departments.ID = tbl_Rooms.Department_ID LEFT OUTER JOIN
                         tbl_Manufacturers ON tbl_Items.Manufacturer_ID = tbl_Manufacturers.ID LEFT OUTER JOIN
                         vw_Ref_Table AS tItemType ON tbl_Items.Type_ID = tItemType.ID LEFT OUTER JOIN
                         vw_RI_Options ON tbl_Room_Items.ID = vw_RI_Options.Room_Item_ID LEFT OUTER JOIN
                         vw_Item_Options ON tbl_Room_Items.ID = vw_Item_Options.Room_Item_ID
WHERE        (tbl_Projects.ID = 2)</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
